{
  "functions": {
    "api/coze-chat.js": {
      "runtime": "nodejs22.x"
    },
    "api/auth/[...path].js": {
      "runtime": "nodejs22.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}